{
  "name": "nezuko-boiler-plate",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:deploy": "vitest --run",
    "test:snapshot": "vitest -u"
