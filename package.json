    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "globals": "^15.12.0",
    "husky": "^9.0.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.33",
